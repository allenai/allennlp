{
    "dataset_reader": {
        "type": "ptb_trees",
        "use_pos_tags": false
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 5,
        "padding_noise": 0,
        "sorting_keys": [
            [
                "tokens",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "constituency_parser",
        "encoder": {
            "type": "lstm",
            "hidden_size": 4,
            "input_size": 2,
            "num_layers": 1
        },
        "feedforward": {
            "activations": "relu",
            "hidden_dims": 4,
            "input_dim": 8,
            "num_layers": 1
        },
        "span_extractor": {
            "type": "endpoint",
            "input_dim": 4
        },
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 2,
                    "trainable": true
                }
            }
        }
    },
    "train_data_path": "allennlp/tests/fixtures/data/example_ptb.trees",
    "validation_data_path": "allennlp/tests/fixtures/data/example_ptb.trees",
    "trainer": {
        "cuda_device": -1,
        "grad_norm": 1,
        "num_epochs": 1,
        "optimizer": {
            "type": "adadelta",
            "lr": 1e-06,
            "rho": 0.95
        },
        "patience": 500
    }
}
