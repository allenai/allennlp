{
    "dataset_reader":{
        "type":"ptb_trees",
        "use_pos_tags": false
    },
    "train_data_path": "allennlp/tests/fixtures/data/example_ptb.trees",
    "validation_data_path": "allennlp/tests/fixtures/data/example_ptb.trees",
    "model": {
      "type": "constituency_parser",
      "text_field_embedder": {
        "token_embedders": {
          "tokens": {
            "type": "embedding",
            "embedding_dim": 2,
            "trainable": true
          }
        }
      },
      "encoder": {
        "type": "lstm",
        "input_size": 2,
        "hidden_size": 4,
        "num_layers": 1
      },
      "feedforward": {
        "input_dim": 8,
        "num_layers": 1,
        "hidden_dims": 4,
        "activations": "relu"
      },
      "span_extractor": {
        "type": "endpoint",
        "input_dim": 4
      }
    },

    "iterator": {
      "type": "bucket",
      "sorting_keys": [["tokens", "num_tokens"]],
      "padding_noise": 0.0,
      "batch_size" : 5
    },
    "trainer": {
      "num_epochs": 1,
      "grad_norm": 1.0,
      "patience": 500,
      "cuda_device": -1,
      "optimizer": {
        "type": "adadelta",
        "lr": 0.000001,
        "rho": 0.95
      }
    }
  }

