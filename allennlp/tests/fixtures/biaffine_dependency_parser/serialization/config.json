{
    "dataset_reader": {
        "type": "universal_dependencies"
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 5,
        "padding_noise": 0,
        "sorting_keys": [
            [
                "words",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "biaffine_parser",
        "arc_representation_dim": 3,
        "encoder": {
            "type": "lstm",
            "hidden_size": 4,
            "input_size": 2,
            "num_layers": 1
        },
        "tag_representation_dim": 3,
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 2,
                    "trainable": true
                }
            }
        }
    },
    "train_data_path": "allennlp/tests/fixtures/data/dependencies.conllu",
    "validation_data_path": "allennlp/tests/fixtures/data/dependencies.conllu",
    "trainer": {
        "cuda_device": -1,
        "grad_norm": 1,
        "num_epochs": 1,
        "optimizer": {
            "type": "adadelta",
            "lr": 1e-06,
            "rho": 0.95
        },
        "patience": 500
    }
}
