{
    "dataset_reader": {
        "type": "atis",
        "database_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/atis/atis.db",
        "num_turns_to_concatenate": 3
    },
    "iterator": {
        "type": "basic",
        "batch_size": 4
    },
    "model": {
        "type": "atis_parser",
        "action_embedding_dim": 10,
        "database_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/atis/atis.db",
        "decoder_beam_search": {
            "beam_size": 5
        },
        "dropout": 0.5,
        "encoder": {
            "type": "lstm",
            "bidirectional": true,
            "hidden_size": 40,
            "input_size": 20,
            "num_layers": 1
        },
        "input_attention": {
            "type": "dot_product"
        },
        "max_decoding_steps": 10,
        "utterance_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 20,
                    "trainable": true
                }
            }
        }
    },
    "train_data_path": "allennlp/tests/fixtures/data/atis/sample.json",
    "validation_data_path": "allennlp/tests/fixtures/data/atis/sample.json",
    "trainer": {
        "cuda_device": -1,
        "num_epochs": 2,
        "optimizer": {
            "type": "sgd",
            "lr": 0.1
        },
        "patience": 5
    },
    "validation_dataset_reader": {
        "type": "atis",
        "database_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/atis/atis.db",
        "keep_if_unparseable": true,
        "num_turns_to_concatenate": 3
    }
}
