{
  "dataset_reader": {
    "type": "wikitables",
    "tables_directory": "/Users/pradeepd/data/wikitables/WikiTableQuestions/",
    "dpd_output_directory": "/Users/pradeepd/data/wikitables/dpd_output/"
  },
  "train_data_path": "/Users/pradeepd/data/wikitables/sample/train.examples",
  "validation_data_path": "/Users/pradeepd/data/wikitables/sample/dev.examples",
  "model": {
    "type": "wikitables_mml_parser",
    "question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 25,
        "trainable": true
      }
    },
    "action_embedding_dim": 50,
    "encoder": {
      "type": "lstm",
      "input_size": 50,
      "hidden_size": 10,
      "num_layers": 1
    },
    "entity_encoder": {
      "type": "boe",
      "embedding_dim": 25,
      "averaged": true
    },
    "max_decoding_steps": 20,
    "decoder_beam_search": {
      "beam_size": 10
    },
    "attention_function": {"type": "dot_product"},
    "tables_directory": "/Users/pradeepd/data/wikitables/WikiTableQuestions/"
  },
  "iterator": {
    "type": "bucket",
    "sorting_keys": [["question", "num_tokens"]],
    "padding_noise": 0.0,
    "batch_size" : 10
  },
  "trainer": {
    "num_epochs": 50,
    "patience": 10,
    "validation_metric": "+denotation_acc",
    "cuda_device": -1,
    "optimizer": {
      "type": "adam",
      "lr": 0.01
    }
  }
}
