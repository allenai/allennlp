{
  "dataset_reader": {
    "type": "wikitables-preprocessed",
    "lazy": false,
    "question_token_indexers": {
      "tokens": {"type": "single_id"}
    }
  },
  "vocabulary": {
    "min_count": {"tokens": 3}
  },
  "train_data_path": "/wikitables_preprocessed/linked_numbers/train.jsonl",
  "validation_data_path": "/wikitables_preprocessed/linked_numbers/validation.jsonl",
  "model": {
    "type": "wikitables_parser",
    "question_embedder": {
      "tokens": {
        "type": "embedding",
        "embedding_dim": 200,
        "trainable": true
      }
    },
    "nonterminal_embedder": {
      "tokens": {
        "type": "embedding",
        "vocab_namespace": "rule_labels",
        "embedding_dim": 50,
        "trainable": true
      }
    },
    "encoder": {
      "type": "lstm",
      "input_size": 400,
      "hidden_size": 100,
      "bidirectional": true,
      "num_layers": 1
    },
    "entity_encoder": {
      "type": "boe",
      "embedding_dim": 200,
      "averaged": true
    },
    "decoder_trainer": {
      "type": "max_marginal_likelihood"
    },
    "decoder_beam_search": {
      "beam_size": 3
    },
    "max_decoding_steps": 20,
    "attention_function": {"type": "dot_product"}
  },
  "iterator": {
    "type": "basic",
    "batch_size" : 1
  },
  "trainer": {
    "num_epochs": 20,
    "patience": 5,
    "cuda_device": 0,
    "validation_metric": "+parse_acc",
    "optimizer": {
      "type": "adam"
    }
  }
}
